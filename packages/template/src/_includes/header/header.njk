      <a href="{{ entry.link }}">
        <span class="drop-ico">{{ entry.icon }}</span>
        {{ entry.name }}
      </a>
    {% else %}
      <a href="{{ entry.link }}">{{ entry.name }}</a>
    {% endif %}
